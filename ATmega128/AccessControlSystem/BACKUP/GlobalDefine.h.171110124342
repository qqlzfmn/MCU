//#include "GlobalDefine.h"

#ifndef _GlobalDefine_H
#define _GlobalDefine_H

//定义LED引脚
#defien led1				PORTA.0			//定义PA0脚为led1输出位
#defien led2				PORTA.1			//定义PA1脚为led2输出位
#defien led3				PORTA.2			//定义PA2脚为led3输出位
#defien led4				PORTA.3			//定义PA3脚为led4输出位

//定义LCD引脚
#define lcd_busy			PINA & BIT(4)	//定义PA4脚为lcd_busy输入位
#define lcd_rx				PORTE.1			//定义PE1脚为lcd_rx输出位
#define lcd_tx				PORTE.0			//定义PE0脚为lcd_tx输出位

//定义指纹模块引脚
#define fingerprint_rx		PORTD.3			//定义PD3脚为fingerprint_rx输出位
#define fingerprint_tx		PORTD.2			//定义PD2脚为fingerprint_tx输出位
#define fingerprint_power	PORTA.7			//定义PA7脚为fingerprint_power输出位

//定义键盘模块引脚
#define keyboard_row1		PORTB.0
#define keyboard_row2		PORTB.1
#define keyboard_row3		PORTB.2
#define keyboard_row4		PORTB.3
#define keyboard_col1		PORTB.4
#define keyboard_col2		PORTB.5
#define keyboard_col3		PORTB.6
#define keyboard_col4		PORTB.7

#endif
